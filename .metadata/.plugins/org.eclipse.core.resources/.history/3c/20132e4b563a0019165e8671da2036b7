package com.ys.reflex;

public class Person {
	 //私有属性
  private String name = "Tom";
  //公有属性
  public int age = 18;
  //构造方法
  public Person() {    
  }
  //私有方法
  private void say(){
      System.out.println("private say()...");
  }
  //公有方法
  public void work(){
      System.out.println("public work()...");
  }
  
  public static void main(String[] args) {
		Person p1 = new Person();
		class c1 = p1.
	}
}
